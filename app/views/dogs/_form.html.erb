<div class="dog-form">
  <%= form_for @dog do |f| %>
    <%= render partial: 'shared/error', locals: {obj: @dog} %>
    <fieldset>
      <legend>Dog Information</legend>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <hr>
      <%= f.label :breed %>
      <%= f.text_field :breed %>
      <hr>
      <%= f.label :age %>
      <%= f.text_field :age %>

      <!--  <h4>Select a dog</h4>-->
      <%#= f.collection_select :dog_id, Employee.all, :id, :name %>
      <%= f.submit 'Create Dog!' %>
    </fieldset>
  <% end %>
</div>
